#include <iostream>

using namespace std;

int main()
{
    int a[10];
    int n;
    cin >> n;

    int i;
    for (i = 0; i < n; ++i) {
        cin >> a[i];
    }

    int j;
    for (i = n - 1; i >= 0; --i) {
        if (a[i] > 0) {
            ++n;
            for (j = n - 1; j > i; --j) a[j] = a[j - 1];
            a[i] = 0;
        }
    }

    for (i = 0; i < n; ++i) 
        cout << a[i];

    return 0;
}
