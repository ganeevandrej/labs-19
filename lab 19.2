#include <iostream>
using namespace std;
int main()
{
    int a[10];
    int n;
    cin >> n;

    for (int i = 0; i < n; ++i) {
        cin >> a[i];
    }

    int ai, j, k, num;

    for (int i = 0; i < n; i++) {
        num = 0;

        for (j = 0; j < n; ++j) { 
            if (a[i] == a[j]) ++num;
        }

        if (num == 2) {
            k = i - 1;
            for (j = i; j < n; ++j)
                if (a[j] != a[i]) {
                    ++k;
                    a[k] = a[j];
                }
            n = k + 1;
            --i;
        }
    }

    cout << n << endl;
    for (int i = 0; i < n; ++i)
        cout << a[i];

    return 0;
}
